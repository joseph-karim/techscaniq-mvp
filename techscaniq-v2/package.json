{
  "name": "techscaniq-v2",
  "version": "2.0.0",
  "description": "Deep Research Agent Platform for PE Due Diligence",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:api": "nodemon --exec tsx src/api/server.ts",
    "dev:orchestrator": "tsx watch src/orchestrator/index.ts",
    "dev:workers": "tsx src/workers.ts",
    "workers": "tsx src/workers.ts",
    "build": "tsc",
    "start": "node dist/start-api.js",
    "start:dev": "tsx start-api.ts",
    "postinstall": "npm run build",
    "test": "vitest",
    "test:queues": "tsx test-queues.ts",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "migrate": "tsx scripts/migrate.ts",
    "research:checkpoint": "tsx scripts/run-with-checkpoint.ts"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.2.0",
    "@google/generative-ai": "^0.24.1",
    "@langchain/anthropic": "^0.3.0",
    "@langchain/community": "^0.3.0",
    "@langchain/core": "^0.3.0",
    "@langchain/langgraph": "^0.2.0",
    "@langchain/openai": "^0.3.0",
    "@supabase/supabase-js": "^2.45.0",
    "axios": "^1.7.0",
    "bullmq": "^5.0.0",
    "cheerio": "^1.0.0",
    "crawlee": "^3.8.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.0",
    "fastify": "^4.29.1",
    "fs-extra": "^11.3.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "openai": "^5.3.0",
    "pdf-parse": "^1.1.1",
    "playwright": "^1.45.0",
    "redis": "^4.6.0",
    "semver": "^7.7.2",
    "uuid": "^10.0.0",
    "xml2js": "^0.6.2",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.0",
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "tsx": "^4.15.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  }
}
